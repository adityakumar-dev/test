{
  "version": 2,
  "buildCommand": "pnpm install && cd frontend && pnpm install && pnpm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "pnpm install",
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    }
  ]
}
